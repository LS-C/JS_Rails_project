<%= form_for(@user) do |f| %>
    First Name: <%= f.text_field :first_name %> <br>
    Last Name: <%= f.text_field :last_name %> <br>
    <%= f.submit %>
<%end%>



<script type="text/javascript">
    $('form').submit(function(e) {
        e.preventDefault()
        var values = $(this).serialize();
        console.log(values)
        posting = $.post('/users', values)
        posting.done(function(data) {
            console.log(data)
        })
    })
</script>
